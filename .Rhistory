# Create server for application
shinyServer(function(input, output) {
output$bar <- renderPlot({
plot <- ggplot(data = pop_states) +
geom_col(
mapping = aes(x = state, y = eval(parse(text = input$type))),
fill = "darksalmon"
) +
labs(title = "Midwestern State Population Demographic", x = "State",
y = input$type)
plot
})
output$scatter <- renderPlotly({
p <- ggplot(
data = perc_midwest,
mapping = aes(x = eval(parse(text = input$xvar)),
y = eval(parse(text = input$yvar)))) +
geom_hex() +
labs(title = "Midwestern States Percentage Comparison", x = input$xvar,
y = input$yvar)
p
})
filtered <- reactive({
ifelse(
input$drink == "All",
menu,
menu %>%
filter(Beverage_Category == input$drink) %>%
select(-Beverage_Category)
)
menu %>%
select(Beverage_Category, Beverage, Beverage_Prep, input$filter)
menu
})
output$table <- renderDataTable({
filtered()
})
})
